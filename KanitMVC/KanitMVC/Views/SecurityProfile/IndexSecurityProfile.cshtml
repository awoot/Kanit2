
@{
    ViewBag.Title = "IndexSecurityProfile";
}

@*<h2>IndexSecurityProfile</h2>*@
<script src="@Url.Content("~/Scripts/CUDS/Setting/SecurityProfile/IndexSecurityProfile.js")" type="text/javascript"></script>
<div class="modal fade" id="ShowDialog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
<div class="row">
    <div class="col-lg-12">
        <!--<h3 class="page-header"><i class="fa fa-file-text-o"></i> Company</h3>-->
        <div class="page-header">
            <div class="col-xs-9 col-sm-9 col-lg-11 marginbuttom15"><a href="/SecurityProfile/CreateSecurityProfile" class="btn btn-primary">Create</a></div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <!--<div class="col-lg-6"></div>-->
            <header class="panel-heading">
                <div class="col-lg-9">Security Profile</div>
                <div class="col-lg-3 padding7"><input id="searchInput" class="form-control" placeholder="Search" type="text"></div>
            </header>
            <div class="table-responsive">
                <table id="tblCompany" class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>No.</th>
                            <th>Profile</th>
                            <th>First Page</th>
                            <th><i class="icon_cogs"></i> Action</th>
                        </tr>
                    </thead>
                    <tbody id="resultSecurityProfile">
                    </tbody>
                </table>
            </div>
            <div class="modal fade" id="ConfirmDelete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <h4 class="modal-title">Delete</h4>
                        </div>
                        <div class="modal-body">

                            Do You Want to Delete?

                        </div>
                        <div class="modal-footer">
                            <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                            <button class="btn btn-danger" type="button" data-dismiss="modal"> Confirm</button>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>  
